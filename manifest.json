{
  "manifest_version": 2,
  "name": "Force Youtube Theme Refresh",
  "version": "1.0",
  "description": "Forces the Youtube theme refresh for most Youtube pages",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/watch?*",
    "*://*.youtube.com/*/player?*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
